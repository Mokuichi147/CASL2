P184Q6	START
	RPUSH
	LAD	GR1,10
	CALL	SUM
	CALL	DISP
	OUT	DATA,LEN
	RPOP
	RET
;
SUM	PUSH	0,GR2
	LAD	GR2,0
LOO	CPA	GR1,=0
	JZE	RE
	ADDL	GR2,GR1
	LAD	GR1,-1,GR1
	JUMP	LOO
RE	LD	GR1,GR2
	POP	GR2
	RET
;
DISP	PUSH	0,GR1
	LAD	GR7,3
	LAD	GR6,0
;
LOOP	LD	GR2,GR1
	AND	GR2,=#000F
	LD	GR3,HEX,GR2
	ST	GR3,DATA,GR7
	SRL	GR1,4
	LAD	GR7,-1,GR7
	CPA	GR7,=-1
	JPL	LOOP
	POP	GR1
	RET
;
HEX	DC	'0123456789ABCDEF'
LEN	DC	4
DATA	DS	4
;
	END