P127Q5	START
	RPUSH
;
	LD	GR1,=1
	LD	GR2,=3
;
	IN	INPUT,LEN
;
	LD	GR0,INPUT
	CPA	GR0,='A'
	JZE	A
	CPA	GR0,='O'
	JZE	O
	CPA	GR0,='X'
	JZE	X
;
	LD	GR3,GR1
	JUMP	FIN
;
A	AND	GR1,GR2
	LD	GR3,GR1
	JUMP	FIN
;
O	OR	GR1,GR2
	LD	GR3,GR1
	JUMP	FIN
;
X	XOR	GR1,GR2
	LD	GR3,GR1
;
FIN	RPOP
	RET
INPUT	DS	256
LEN	DS	1
	END