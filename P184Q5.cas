P184Q5	START
	RPUSH
	LAD	GR1,#1234
	CALL	DISP
	OUT	DATA,LEN
	RPOP
	RET
;
DISP	PUSH	0,GR1
	LAD	GR7,3
	LAD	GR6,0
;
LOOP	LD	GR2,GR1
	AND	GR2,=#000F
	LD	GR3,HEX,GR2
	ST	GR3,DATA,GR7
	SRL	GR1,4
	LAD	GR7,-1,GR7
	CPA	GR7,=-1
	JPL	LOOP
	POP	GR1
	RET
;
